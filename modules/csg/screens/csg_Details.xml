<?xml version="1.0" encoding="utf-8"?>
<screen>
	<screenTitle>CSG Request Details</screenTitle>
	<box>
		<id>csgDetails_Box</id>
		<style>
			<position>absolute</position>
			<backgroundColor>Secondary</backgroundColor>
			<left>0px</left>
			<top>0px</top>
			<width>600px</width>
			<height>60px</height>
			<color>Primary</color>
			<fontWeight>bold</fontWeight>
			<zIndex>1</zIndex>
		</style>
		<label>
			<text>Request</text>
			<style>
				<top>10px</top>
				<left>25px</left>
				<zIndex>2</zIndex>
			</style>
		</label>
		<div>
			<id>csgDetails_Request</id>
			<title>Request number</title>
			<style>
				<top>30px</top>
				<left>25px</left>
				<width>150px</width>
				<zIndex>2</zIndex>
				<color>Primary</color>
				<backgroundColor>Tertiary</backgroundColor>
			</style>
		</div>
		<label>
			<text>Owner</text>
			<style>
				<top>10px</top>
				<left>225px</left>
				<zIndex>2</zIndex>
			</style>
		</label>
		<div>
			<id>csgDetails_Owner</id>
			<title>Transaction Submitter</title>
			<style>
				<top>30px</top>
				<left>225px</left>
				<width>150px</width>
				<zIndex>2</zIndex>
				<color>Primary</color>
				<backgroundColor>Tertiary</backgroundColor>
			</style>
		</div>
		<button>
			<id>csgDetails_back</id>
			<text>Back</text>
			<style>
				<left>500px</left>
				<top>30px</top>
			</style>
		</button>
	</box>
	<div>
		<id>csgDetails_tab</id>
		<style>
			<top>100px</top>
			<left>0px</left>
		</style>
	</div>
	<table>
		<id>csgDetails_Table</id>
		<style>
			<left>0px</left>
			<top>0px</top>
			<width>600px</width>
		</style>
	</table>
	<table>
		<id>csgDetails_HistoryTable</id>
		<style>
			<left>0px</left>
			<top>0px</top>
			<width>600px</width>
		</style>
	</table>
</screen>
